{
    "name": "enebe-nb/phergie-irc-plugin-react-tell",
    "description": "Phergie plugin for send messages to users next time they are seen.",
    "keywords": [ "irc", "bot", "react", "plugin" ],
    "license": "BSD-2-Clause",
    "support": {
        "issues": "https://github.com/enebe-nb/phergie-irc-plugin-react-tell/issues",
        "source": "https://github.com/enebe-nb/phergie-irc-plugin-react-tell"
    },
    "require": {
        "phergie/phergie-irc-bot-react": "1.*",
        "phergie/phergie-irc-plugin-react-command": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "phake/phake": "2.*"
    },
    "suggest": {
        "phergie/phergie-irc-plugin-react-eventfilter": "Filter events to listen especific channels, users and more"
    },
    "autoload": {
        "psr-4": {
            "EnebeNb\\Phergie\\Plugin\\Tell\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EnebeNb\\Phergie\\Tests\\Plugin\\Tell\\": "tests"
        }
    }
}
